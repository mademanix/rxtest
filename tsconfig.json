{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "lib": ["es2015", "dom"],
      "moduleResolution": "node",
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "./js/build/",
      "sourceMap": true
    },
    "include": ["./js/src/*"],
    "exclude": ["node_modules"]
}